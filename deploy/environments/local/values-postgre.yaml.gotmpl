global:
    postgresql:
        auth:
            postgresPassword: {{ env "POSTGRES_ADMIN_PASSWORD" }}
            username: {{ env "POSTGRES_USER" }}
            password: {{ env "POSTGRES_USER_PASSWORD" }}
            database: {{ env "POSTGRES_USER_DB" }}

primary:
    service:
        type: NodePort
        nodePorts:
            postgresql: 30100

    persistence:
        size: {{ env "POSTGRES_PV_SIZE" }}
